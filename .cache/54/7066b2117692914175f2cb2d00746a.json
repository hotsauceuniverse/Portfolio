p: 20px;\r\n  text-decoration: underline;\r\n  text-align: center;\r\n  background-color: powderblue;\r\n  border-radius: 35px;\r\n}\r\n\r\n.capability-section {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  margin-top: 20px;\r\n  width: 1349px;\r\n  margin: 30px auto;\r\n}\r\n\r\n.capability-section .capability {\r\n  display: flex;\r\n  margin-bottom: 0px;\r\n}\r\n\r\n.capability-section .capability .capability-icon {\r\n  width: 100px;\r\n  height: 100px;\r\n  padding: 15px;\r\n  border: 2px solid black;\r\n  border-radius: 50px;\r\n  margin: auto;\r\n}\r\n\r\n.recent-works {\r\n  display: flex;\r\n  flex-direction: column;\r\n  width: 1349px;\r\n  margin: auto;\r\n}\r\n\r\n.recent-works .article {\r\n  display: flex;\r\n  flex-direction: row;\r\n  margin-bottom: 70px;\r\n}\r\n\r\n.recent-works .article .thumbnail1,\r\n.recent-works .article .thumbnail2,\r\n.recent-works .article .thumbnail3,\r\n.recent-works .article .thumbnail4,\r\n.recent-works .article .thumbnail5 {\r\n  display: flex;\r\n  width: 200px;\r\n  height: 200px;\r\n  margin-left: 30px;\r\n}\r\n\r\nh3 {\r\n  font-size: 50px;\r\n  margin-left: 30px;\r\n}\r\n\r\n.article-comment {\r\n  font-size: 20px;\r\n  margin-left: 30px;\r\n}\r\n\r\n.more {\r\n  font-size: 15px;\r\n  margin-left: 30px;\r\n  color: inherit; /* 하이퍼링크의 색상 제거 */\r\n}\r\n\r\n.more:hover {\r\n  background-color: powderblue;\r\n  transition: 0.5s;\r\n}\r\n\r\n/* CONTACT ME */\r\n\r\n.contact {\r\n  font-size: 50px;\r\n  font-weight: 700;\r\n  margin-top: 20px;\r\n  text-decoration: underline;\r\n  text-align: center;\r\n  background-color: powderblue;\r\n  border-radius: 35px;\r\n}\r\n\r\n.link{\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.github, .instagram, .mail {\r\n  width: 100px;\r\n  height: 100px;\r\n  margin: 20px;\r\n  margin-bottom: 50px;\r\n}"},"lineCount":352}},"error":null,"hash":"3602590cc2443cf239cccab0e2399b89","cacheData":{"env":{}}}