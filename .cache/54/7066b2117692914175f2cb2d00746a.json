 background-color: #f6f5f0;\r\n}\r\n\r\nheader.shrink div ul li {\r\n  line-height: 10em;\r\n}\r\n\r\nheader > .inner {\r\n  height: 120px;\r\n}\r\n\r\nheader #logo {\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0; /*부모요소 기준으로 위치 찍기 */\r\n}\r\n\r\nheader .main-menu {\r\n  position: absolute;\r\n  bottom: 0;\r\n  right: 0;\r\n  display: flex;\r\n}\r\n\r\nheader .main-menu .item {\r\n  cursor: pointer;\r\n}\r\n\r\nheader .main-menu .item .item__name{\r\n  padding: 10px ;\r\n  font-family: Arial, sans-serif;\r\n  font-size: 20px;\r\n}\r\n\r\nheader .main-menu .item .scroll {\r\n  color: inherit; /* 하이퍼링크의 색상 제거 */\r\n}\r\n\r\nheader .main-menu .item .item__name:hover{\r\n  color: powderblue;\r\n  transition: 0.5s;\r\n}\r\n\r\nheader ul.main-menu li{\r\n  position: relative;\r\n}\r\n\r\nheader ul.main-menu li::before{ /*가상 요소 선택자*/\r\n  content: \"\";\r\n  display: block;\r\n  width: 1px;\r\n  height: 12px;\r\n  background-color: black;\r\n  position: absolute;\r\n  top: 0;\r\n  bottom: 0;\r\n  margin: auto 0;\r\n}\r\n\r\nheader ul.main-menu li:first-child:before{ /*가상 클래스 선택자*/\r\n  display: none;\r\n}\r\n\r\nheader .title {\r\n  position: absolute;\r\n  top: 10px;\r\n  right: 0;\r\n  display: flex;\r\n  color: gray;\r\n}\r\n\r\nheader .title a{\r\n  font-size: 40px;\r\n  padding: 11px 16px;\r\n  display: block;\r\n}\r\n\r\n/* SECTION */\r\n\r\n.dashboard {\r\n  font-size: 30px;\r\n  align-items: center;\r\n  margin-top: 120px;\r\n  display: flex;\r\n  width: 1349px;\r\n  margin: 120px auto 0; /*화면 중앙으로 모아주는 부분*/\r\n}\r\n\r\n.main {\r\n  width: 530px;\r\n  height: 537px;\r\n  margin-right: 60px;\r\n}\r\n\r\n.title1 {\r\n  font-size: 50px;\r\n  font-weight: 700;\r\n  margin-top: 20px;\r\n  text-decoration: underline;\r\n  text-align: center;\r\n  background-color: powderblue;\r\n  border-radius: 35px;\r\n}\r\n\r\n/* PICTURE */\r\n\r\n.picture .promotion {\r\n  height: 550px;\r\n  background-color: #f6f5ef;  \r\n  position: relative;\r\n}\r\n\r\n.picture .promotion